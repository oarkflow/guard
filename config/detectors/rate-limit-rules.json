{
    "detector": {
        "name": "rate_limit_detector",
        "enabled": true,
        "priority": 90,
        "parameters": {
            "window_size": "1m",
            "max_requests": 100,
            "key_template": "rate_limit:{ip}",
            "burst_allowed": 10,
            "cleanup_period": "5m"
        }
    },
    "action_rules": [
        {
            "name": "Rate Limit Warning",
            "description": "Warn when approaching rate limit",
            "min_severity": 2,
            "min_confidence": 0.6,
            "actions": [ "warning_action" ],
            "threat_tags": [ "rate_limit" ],
            "priority": 85,
            "enabled": true
        },
        {
            "name": "Rate Limit Block",
            "description": "Block IPs that exceed rate limits",
            "min_severity": 5,
            "min_confidence": 0.8,
            "actions": [ "incremental_block_action" ],
            "threat_tags": [ "rate_limit", "ddos" ],
            "priority": 85,
            "enabled": true
        }
    ]
}
